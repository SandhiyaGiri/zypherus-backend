{
  "name": "@zypherus/prompt-kit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint ./src --ext .ts",
    "test": "vitest",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@zypherus/shared-types": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
